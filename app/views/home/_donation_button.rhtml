<% form_tag 'https://www.paypal.com/cgi-bin/webscr' do %>
  <div class="centralise">
  <%= hidden_field_tag 'cmd', '_s-xclick' %>
  <%= image_submit_tag 'https://www.paypal.com/en_US/i/btn/x-click-but21.gif',
    :border =>0, :alt => "Make payments with Paypal - it's fast, free and secure!" %>
  <%= hidden_field_tag 'encrypted', 
    "-----BEGIN PKCS7----------END PKCS7-----
"
%>
</div>
<% end %>
